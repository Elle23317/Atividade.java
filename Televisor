public class Televisor{

//Atributos
    private int canal;
    private int volume;
    private boolean ligado;

public Televisor(){
    this.canal = 1;
    this.volume = 10;
    this.ligado = false;

}

public void ligar(){
    ligado = true;
    System.out.println("Televisor ligado");
}

public void desligar(){
    ligado = false;
    System.out.println("Televisor desligado");
}

public void aumentarVolume(){
    if(ligado = true && volume < 100){
        volume ++;
    
    }
}

public void diminuirVolume(){
    if(ligado = true && volume > 0){
        volume --;

    }
}

public void trocarCanal( int novoCanal){
    if(ligado = true && novoCanal > 0 && canal != novoCanal){
        canal = novoCanal;
    }

}

public String toString(){
    return String.format("Televisor{ canal: " + canal + "Volume:" + "Ligado:" + (ligado ? "Sim" : "NÃ£o") + "}");
    }
}
